{
    "builds": [
        {
            "src": "dictionary_site/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dictionary_site/wsgi"
        }
    ]
}